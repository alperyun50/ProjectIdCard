@model List<ProjectIdCard.Models.Kitap>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index5</title>
    <link href="~/twitter-bootstrap/css/bootstrap.css" rel="stylesheet"/>
    <script src="~/twitter-bootstrap/js/bootstrap.js"></script>
</head>
<body>
    <div>
        <h4>Kitaplar</h4>
        <table class="table table-bordered">
            @foreach (var k in Model)
            {
                <tr>
                    <td>@k.ID.</td>
                    <td>@k.KitapAd</td>
                    <td>@k.Yazar</td>
                </tr>
            }
        </table>
        <button class="btn btn-warning">Tablo listesi</button>
    </div>
</body>
</html>
 